
        // create an array of length 5
        int[] age = new int[5];

        // access elements using of the array
        for (int i = 0; i < 5; ++i) {
            System.out.println(age[i]);
        }
-----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

        // create an array
        int[] age = {12, 4, 5, 2, 5};

        // access elements of the array
        for (int i = 0; i < 5; ++i) {
            System.out.println("Element at index " + i +": " + age[i]);
        }
